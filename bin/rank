#!/usr/bin/env julia

require("Pagerank")
using Pagerank

alpha = 0.85
accuracy = 0.00001
edgelist = ARGS[1]

rank = Rank(alpha, accuracy, edgelist)
r, origin_idx = stationary_distribution(rank)

projected_idx = map(x -> x[2], sort(reverse(pairs(origin_idx))))
count = length(r)

for i in 1:count
  println("$(projected_idx[i])	$(r[i])")
end

