#!/usr/bin/env julia

require("Pagerank")
using Pagerank

alpha = 0.85
accuracy = 0.00001
adjacencies = ARGS[1]

rank = Rank{Float32,Int64}(alpha, accuracy, adjacencies)
r = stationary_distribution(rank)
count = length(r)

for i in 1:count
  print(i)
  print("	")
  println(@sprintf("%0.20f", r[i]))
end

